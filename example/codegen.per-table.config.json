{
  "typesPath": "./database.types.ts",
  "relativeSupabasePath": "./supabase",
  "supabaseExportName": "supabase",
  "output": {
    "tablesDir": {
      "hooks": "./generated/tables/{{table}}/hooks.ts",
      "schemas": "./generated/tables/{{table}}/schemas.ts"
    },
    "functionsDir": {
      "hooks": "./generated/functions/{{function}}/hooks.ts",
      "schemas": "./generated/functions/{{function}}/schemas.ts"
    },
    "enums": {
      "schemas": "./generated/enums/schemas.ts"
    }
  }
}
